<UserControl x:Class="Maeily_Windows.Controls.CalendarContent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Maeily_Windows.Controls"
             mc:Ignorable="d"
             d:DesignHeight="45" d:DesignWidth="250">
    <StackPanel Orientation="Horizontal" Margin="5">
        <Rectangle x:Name="retangle" Height="10" Width="10" Margin="50, 0, 0, 0" />
        <TextBlock x:Name="TxtContent" VerticalAlignment="Center" Margin="10, 0, 0, 0" />
    </StackPanel>
</UserControl>