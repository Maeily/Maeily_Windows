      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Maeily_Windows"
      mc:Ignorable="d" 
      d:DesignHeight="520" d:DesignWidth="312"
      Title="Channel_Settings">
    <Grid>
        <StackPanel Background="Transparent" Height="400" Margin="0, 60" Name="CalendarList">
            <UniformGrid>
            </UniformGrid>
        </StackPanel>
        <Button Height="60" Width="60" Margin="210, 400, 0, 0" Background="Transparent" BorderBrush="Transparent" Click="Button_Click">
            <Image Source="Resources/AddBtn.png"/>
        </Button>
        <TextBox x:Name="txtbox" Width="150" Height="20" Margin="-40, 400, 0, 0" Visibility="Hidden"/>
    </Grid>
</Page>
